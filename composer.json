{
    "name": "sbooker/event-loop-worker",
    "description": "ReactPHP's event loop based worker",
    "keywords": ["event loop", "worker", "background process"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey Knigin",
            "email": "sergey.knigin.dev@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.4",
        "ext-pcntl": "*",
        "psr/log": "^1.0",
        "react/event-loop": "^1.1",
        "sbooker/console": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Sbooker\\EventLoopWorker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\Sbooker\\EventLoopWorker\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
